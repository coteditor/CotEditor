[
  {
    "beginString" : "//[ \\t]*(TODO:|FIXME:)[ \\t](.+)",
    "description" : "TODO/FIXME",
    "keyString" : "$1 $2",
    "kind" : "mark"
  },
  {
    "beginString" : "//[ \\t]*MARK:[ \\t]*(?:-[\\t ]+)?(.*)",
    "description" : "MARK",
    "keyString" : "$1",
    "kind" : "mark"
  },
  {
    "beginString" : "//[ \\t]*MARK:[ \\t]*-",
    "description" : "separator",
    "keyString" : "",
    "kind" : "separator"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*((?:class|static)[ \\t]+)?func[ \\t]*(\\w*)(?:<[^>]+>)?\\((.*)\\)",
    "description" : "function",
    "keyString" : "$1$2func $3($4)",
    "kind" : "function"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*(class )?subscript[ \\t]*\\((.*)\\)",
    "description" : "subscript",
    "keyString" : "$1$2subscript($3)",
    "kind" : "function"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*(class )?var[ \\t]*(\\w*)(?=[ \\t]*:[ \\t]*\\w*\\s*\\{)",
    "description" : "var",
    "keyString" : "$1$2var $3",
    "kind" : "value"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*actor[ \\t]+(\\w+)(?=[\\s:][^\\w])",
    "description" : "actor",
    "keyString" : "$1actor $2",
    "kind" : "container"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*class[ \\t]+(\\w+)(?=[\\s:][^\\w])",
    "description" : "class",
    "keyString" : "$1class $2",
    "kind" : "container"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*deinit\\b",
    "description" : "deinitializer",
    "keyString" : "$1deinit",
    "kind" : "function"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*enum[ \\t]+(\\w+)",
    "description" : "enum",
    "keyString" : "$1enum $2",
    "kind" : "container"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*extension[ \\t]+(\\w+)",
    "description" : "extension",
    "keyString" : "$0extension $1",
    "kind" : "container"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*init(\\??)[\\t ]*\\((.*)\\)",
    "description" : "initializer",
    "keyString" : "$1init$2($3)",
    "kind" : "function"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*protocol[ \\t]+(\\w+)",
    "description" : "protocol",
    "keyString" : "$1protocol $2",
    "kind" : "container"
  },
  {
    "beginString" : "^([ \\t]*)(?:@?[a-zA-Z_]+ )*struct[ \\t]+(\\w+)",
    "description" : "struct",
    "keyString" : "$1struct $2",
    "kind" : "container"
  }
]