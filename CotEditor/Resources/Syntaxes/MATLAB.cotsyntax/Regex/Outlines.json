    "template" : "$1$3",
    "kind" : "container"
  },
  {
    "pattern" : "^([\\t ]*)function[\\t ]+(\\w+)\\(",
    "template" : "$1$2",
    "kind" : "function"
  },
  {
    "pattern" : "^([\\t ]*)methods(?:[\\t ]+(\\(.+\\)))?",
    "template" : "$1methods $2",
    "kind" : "function"
  },
  {
    "pattern" : "^([\\t ]*)properties(?:[\\t ]+(\\(.+\\)))?",
