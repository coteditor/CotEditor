{
  "comment" : {
    "blocks" : [
      {
        "begin" : "/*",
        "end" : "*/"
      }
    ],
    "inlines" : [
      {
        "begin" : "--"
      }
    ]
  },
  "lexicalRules" : {
    "delimiterEscapeRule" : "doubleDelimiter"
  },
  "indentation" : {
    "blockDelimiters" : [
      {
        "begin" : "BEGIN",
        "end" : "END",
        "ignoreCase" : true
      },
      {
        "begin" : "CASE",
        "end" : "END",
        "ignoreCase" : true
      }
    ]
  }
}