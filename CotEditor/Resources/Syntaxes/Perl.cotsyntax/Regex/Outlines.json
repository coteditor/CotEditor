[
  {
    "beginString" : "^([\\t ]*)(sub)[ ]*([a-z][a-z0-9_]*)[^{].*\\{",
    "description" : "sub-routine",
    "ignoreCase" : true,
    "keyString" : "$1$2 $3 {}"
  },
  {
    "beginString" : "^[ ]*(\\@ISA)([^;]*);",
    "description" : "@ISA",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "$1 $2"
  },
  {
    "beginString" : "^[ ]*(package)[ ]*([a-z][a-z0-9_]*).*$",
    "description" : "package",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "$1 $2"
  }
]