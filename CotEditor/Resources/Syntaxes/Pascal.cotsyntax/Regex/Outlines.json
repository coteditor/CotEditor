[
  {
    "beginString" : "(^\\s*)(?:\\T)(?:.*)(?<!\\:)\\=(?:.*)",
    "bold" : true,
    "description" : "Types (start with T)",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "type $0"
  },
  {
    "beginString" : "\\{(?:.*)\\$(?:.*)(?:region)(?:.*)\\'(.*)\\'\\}",
    "bold" : true,
    "description" : "Region",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "($1)"
  },
  {
    "beginString" : "^([ \\t]*)(?:[a-z]* )*function(.*)(?:\\;|\\()",
    "description" : "Function",
    "ignoreCase" : true,
    "keyString" : "$2"
  },
  {
    "beginString" : "^([ \\t]*)(?:[a-z]* )*procedure(.*)(?:\\;|\\()",
    "description" : "Procedure",
    "ignoreCase" : true,
    "keyString" : "$2"
  },
  {
    "beginString" : "^\\s*(implementation)",
    "bold" : true,
    "description" : "Implementation",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "---- $1"
  },
  {
    "beginString" : "^\\s*(interface)",
    "bold" : true,
    "description" : "Interface",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "---- $1"
  },
  {
    "beginString" : "^\\s*(type)",
    "bold" : true,
    "description" : "Type",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "---- $1"
  },
  {
    "beginString" : "^\\s*(uses)",
    "bold" : true,
    "description" : "Uses",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "---- $1"
  },
  {
    "beginString" : "unit\\W(.*)(?:\\;)$",
    "bold" : true,
    "description" : "Unit",
    "ignoreCase" : true,
    "italic" : true,
    "keyString" : "$1",
    "underline" : true
  }
]